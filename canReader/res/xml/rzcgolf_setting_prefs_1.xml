<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory android:title="驾驶员辅助系统" >
        <SwitchPreference
            android:key="LANE_DEPARTURE"
            android:summaryOff="OFF"
            android:summaryOn="ON"
            android:title="车道保持辅助系统设定" />
        <SwitchPreference
            android:key="DRIVER_ALERT_SYSTEM"
            android:summaryOff="OFF"
            android:summaryOn="ON"
            android:title="疲劳驾驶识别系统" />
        <SwitchPreference
            android:key="LAST_DISTANCE_SELECTED"
            android:summaryOff="OFF"
            android:summaryOn="ON"
            android:title="上次选择的车距" />
        <SwitchPreference
            android:key="FRONT_ASSIST_ACTIVE"
            android:summaryOff="OFF"
            android:summaryOn="ON"
            android:title="前部辅助系统激活" />
        <SwitchPreference
            android:key="FRONT_ASSIST_ADVANCE_WARNING"
            android:summaryOff="OFF"
            android:summaryOn="ON"
            android:title="前部辅助系统预警" />
        <SwitchPreference
            android:key="FRONT_ASSIST_DISPLAY_DISTANCHE_WARNING"
            android:summaryOff="OFF"
            android:summaryOn="ON"
            android:title="显示距离报警" />
        <ListPreference
            android:entries="@array/rzc_acc_driver_entries"
            android:entryValues="@array/rzc_acc_driver_values"
            android:key="ACC_DRIVER_PROGRAM"
            android:persistent="false"
            android:title="ACC-行驶程序" />
        <ListPreference
            android:entries="@array/rzc_acc_distance_entries"
            android:entryValues="@array/rzc_acc_distance_values"
            android:key="ACC_DISTANCE"
            android:persistent="false"
            android:title="ACC-车距" />
    </PreferenceCategory>

